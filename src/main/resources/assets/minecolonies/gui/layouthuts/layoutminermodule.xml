<window xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/ldtteam/BlockUI/version/main/src/main/resources/assets/blockui/gui/block_ui.xsd"
        size="190 244" pause="false">
    <image source="minecolonies:textures/gui/builderhut/builder_paper.png" size="190 244"/>
    <text size="130 11" pos="30 14" textalign="MIDDLE" color="black"
          label="$(com.minecolonies.coremod.gui.workerhuts.minernodelist):"/>
