        <box size="100% 28" linewidth="1">
            <text id="lvl" size="50 13" pos="5 2" textalign="MIDDLE_LEFT" color="black"/>
            <button id="changeToLevel" size="70 13" pos="30 2"
                    label="$(com.minecolonies.coremod.gui.workerhuts.minerminenode)"/>
            <button id="repair" size="50 13" pos="103 2"
                    label="$(com.minecolonies.coremod.gui.workerhuts.minerrepairnode)"/>

            <text id="nONodes" size="90 12" pos="60 16" textalign="MIDDLE_LEFT" color="black"/>
            <text id="yLevel" size="50 12" pos="5 16" textalign="MIDDLE_LEFT" color="black"/>
        </box>
    </list>
</window>
